{
  "name": "andimar/cards",
  "description": "This is a flash card plugin for wordpress",
  "license":"MIT",
  "type": "wordpress-plugin",
  "authors":[
      {
          "name":"Andrea Marzilli",
          "email":"andimar@gmail.com"
      }
  ],
  "require": {
    "php": ">=7.1"    
  },
  "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
        "Andimar\\Cards\\"       : "src/",
        "Andimar\\Cards\\Test\\" : "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  }
}
